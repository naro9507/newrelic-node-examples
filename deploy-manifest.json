{
  "version": 1,
  "framework": { "name": "custom-server", "version": "1.0.0" },
  "routes": [
    { "path": "/*.*", "target": { "kind": "Static" }, "fallback": { "kind": "Compute", "src": "default" } },
    { "path": "/*",   "target": { "kind": "Compute", "src": "default" } }
  ],
  "computeResources": [
    { "name": "default", "runtime": "nodejs18.x", "entrypoint": "server.js" }
  ]
}
